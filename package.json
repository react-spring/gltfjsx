{
  "name": "gltfjsx",
  "version": "6.5.2",
  "description": "GLTF to JSX converter",
  "prettier": "@alienfast/prettier-config",
  "scripts": {
    "build": "rollup -c",
    "build:ide": "echo 'tsc -b' && tsc -b",
    "lint:fix": "eslint . --cache --fix",
    "test": "node src/test"
  },
  "type": "module",
  "keywords": [
    "gltf",
    "jsx",
    "react",
    "fiber",
    "three",
    "threejs",
    "webp"
  ],
  "author": "Paul Henschel",
  "maintainers": [
    "Max Rusan"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmndrs/gltfjsx.git"
  },
  "bugs": {
    "url": "https://github.com/pmndrs/gltfjsx/issues"
  },
  "homepage": "https://github.com/pmndrs/gltfjsx#readme",
  "bin": "./dist/cli.js",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@gltf-transform/core": "4.1.1",
    "@gltf-transform/extensions": "4.1.1",
    "@gltf-transform/functions": "4.1.1",
    "@node-loader/babel": "^2.1.0",
    "draco3dgltf": "^1.5.7",
    "is-var-name": "^2.0.0",
    "keyframe-resample": "^0.1.0",
    "meow": "^13.2.0",
    "meshoptimizer": "^0.22.0",
    "prettier": "3.4.2",
    "read-pkg-up": "^11.0.0",
    "three": "0.172.0"
  },
  "optionalDependencies": {
    "jsdom": "^26.0.0",
    "jsdom-global": "^3.0.2",
    "libvips": "0.0.2",
    "sharp": "^0.33.5"
  },
  "x-old-resolutions": {
    "sharp": "<0.33.0",
    "@gltf-transform/core": "4.0.8",
    "@gltf-transform/extensions": "4.0.8",
    "@gltf-transform/functions": "4.0.8"
  },
  "devDependencies": {
    "@alienfast/eslint-config": "^5.1.21",
    "@alienfast/prettier-config": "^1.0.0",
    "@alienfast/tsconfig": "^1.0.1",
    "@babel/core": "7.26.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "7.26.3",
    "@babel/plugin-transform-parameters": "7.25.9",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/plugin-transform-template-literals": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/draco3dgltf": "^1.4.3",
    "@types/three": "^0.172.0",
    "chalk": "^5.4.1",
    "eslint": "^9.18.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "lint-staged": "^15.4.1",
    "prettier": "^3.4.2",
    "rollup": "^4.31.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-terser": "^7.0.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "lint-staged": {
    "*.{json,md,gql}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx,js}": [
      "eslint . --cache --ext .js,.ts,.tsx --fix",
      "git add"
    ]
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/react-three-fiber"
  },
  "packageManager": "yarn@4.6.0"
}
